<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.example.android.myscribe">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUp"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.android.SignUp" />

                <categeory android:name="android.intent.categeory.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OtpVerification"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.android.OtpVerification" />

                <categeory android:name="android.intent.categeory.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserDetailsInput"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.android.UserDetailsInput" />

                <categeory android:name="android.intent.categeory.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForgotPassword"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.example.android.ForgotPassword" />

                <categeory android:name="android.intent.categeory.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".Test" />
        <activity android:name=".AadhaarNotLinked"></activity>
    </application>

</manifest>